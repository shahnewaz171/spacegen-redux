{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb", "airbnb/hooks", "prettier"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "react/jsx-filename-extension": "off",
        "arrow-body-style": "off",
        "no-console": "off",
        "no-multi-spaces":"error",
        "no-unused-vars": "off",
        "import/prefer-default-export": "off",
        "no-param-reassign": "off",
        "no-unused-expressions": "off"
    }
}
