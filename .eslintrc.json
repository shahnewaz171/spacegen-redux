{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
  },
  "extends": ["airbnb", "airbnb/hooks", "prettier"],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
      "react/jsx-filename-extension": "off",
      "arrow-body-style": "off",
      "no-console": "off",
      "no-multi-spaces":"error",
      "no-unused-vars": "off",
      "import/prefer-default-export": "off",
      "no-param-reassign": "off",
      "no-unused-expressions": "off",
      "react/self-closing-comp": ["error", {
          "component": true,
          "html": true
      }],
      "jsx-a11y/label-has-associated-control": [ "error", {
          "required": {
            "some": [ "nesting", "id"  ]
          }
        }],
        "jsx-a11y/label-has-for": [ "off", {
          "required": {
            "some": [ "nesting", "id"  ]
          }
        }],
        "react/prop-types": 0,
        "camelcase": "off",
        "array-callback-return": 0,
        "no-else-return": 0,
        "consistent-return": 0,
        "eqeqeq": 0,
        "prefer-template": 0
  }
}
